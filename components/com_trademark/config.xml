<?xml version="1.0" encoding="utf-8"?>
<config>
    <fieldset label="COM_TRADEMARK" name="trademark">
              <field name="applicant_name" type="text"
                
            label="COM_TRADEMARK_FORM_LBL_APPLICANT_NAME"
            description="COM_TRADEMARK_FORM_DESC_APPLICANT_NAME" 
                
                
                 />      <field name="doc_attachment" type="media"
                
            label="COM_TRADEMARK_FORM_LBL_DOC_ATTACHMENT"
            description="COM_TRADEMARK_FORM_DESC_DOC_ATTACHMENT" 
                
                
                 />      <field name="doc_address" type="textarea"
                
            label="COM_TRADEMARK_FORM_LBL_DOC_ADDRESS"
            description="COM_TRADEMARK_FORM_DESC_DOC_ADDRESS" 
                
                
                
            cols="100"
            rows="8" />      <field name="type" type="text"
                
            label="COM_TRADEMARK_FORM_LBL_TYPE"
            description="COM_TRADEMARK_FORM_DESC_TYPE" 
                
                
                 />      <field name="all_directors_fullname" type="textarea"
                
            label="COM_TRADEMARK_FORM_LBL_ALL_DIRECTORS_FULLNAME"
            description="COM_TRADEMARK_FORM_DESC_ALL_DIRECTORS_FULLNAME" 
                
                
                
            cols="100"
            rows="3" />      <field name="signatory_fullname" type="text"
                
            label="COM_TRADEMARK_FORM_LBL_SIGNATORY_FULLNAME"
            description="COM_TRADEMARK_FORM_DESC_SIGNATORY_FULLNAME" 
                
                
                 />      <field name="signatory_phoneno" type="text"
                
            label="COM_TRADEMARK_FORM_LBL_SIGNATORY_PHONENO"
            description="COM_TRADEMARK_FORM_DESC_SIGNATORY_PHONENO" 
                
                
                 />
		<field name="signatory_email" type="email" label="Signatory Email" description="" validate="email"   />      <field name="signatory_nationality" type="text"
                
            label="COM_TRADEMARK_FORM_LBL_SIGNATORY_NATIONALITY"
            description="COM_TRADEMARK_FORM_DESC_SIGNATORY_NATIONALITY" 
                
                
                 />      <field name="signatory_gaurdian_name" type="text"
                
            label="COM_TRADEMARK_FORM_LBL_SIGNATORY_GAURDIAN_NAME"
            description="COM_TRADEMARK_FORM_DESC_SIGNATORY_GAURDIAN_NAME" 
                
                
                 />      <field name="signatory_address" type="text"
                
            label="COM_TRADEMARK_FORM_LBL_SIGNATORY_ADDRESS"
            description="COM_TRADEMARK_FORM_DESC_SIGNATORY_ADDRESS" 
                
                
                 />      <field name="signatory_age" type="integer"
                
            label="COM_TRADEMARK_FORM_LBL_SIGNATORY_AGE"
            description="COM_TRADEMARK_FORM_DESC_SIGNATORY_AGE" 
                
                
                
            first="18"
            last="60"
            step="1" />      <field name="trademark_name" type="text"
                
            label="COM_TRADEMARK_FORM_LBL_TRADEMARK_NAME"
            description="COM_TRADEMARK_FORM_DESC_TRADEMARK_NAME" 
                
                
                 />      <field name="trademark_desc" type="textarea"
                
            label="COM_TRADEMARK_FORM_LBL_TRADEMARK_DESC"
            description="COM_TRADEMARK_FORM_DESC_TRADEMARK_DESC" 
                
                
                
            cols="100"
            rows="8" />      <field name="trademark_state" type="list"
                
            label="COM_TRADEMARK_FORM_LBL_TRADEMARK_STATE"
            description="COM_TRADEMARK_FORM_DESC_TRADEMARK_STATE" 
                
                
                
            multiple="true">
                <option value="manufacturer">Manufacturer</option>
                <option value="serviceprovider">Service Provider</option>
                <option value="trader">Trader</option>
        </field>      <field name="service_address" type="textarea"
                
            label="COM_TRADEMARK_FORM_LBL_SERVICE_ADDRESS"
            description="COM_TRADEMARK_FORM_DESC_SERVICE_ADDRESS" 
                
                
                
            cols="100"
            rows="8" />
    </fieldset>
    <fieldset 
        name="permissions"
	description="JCONFIG_PERMISSIONS_DESC"
	label="JCONFIG_PERMISSIONS_LABEL">

		<field 
                    name="rules" type="rules" component="com_trademark"
                    class="inputbox" filter="rules" validate="rules"
                    label="JCONFIG_PERMISSIONS_LABEL"
                    section="component" />
	</fieldset>
</config>
